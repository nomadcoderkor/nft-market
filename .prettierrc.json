{

  "singleQuote": true,

  "printWidth": 80,

  "tabWidth": 2,

  "useTabs": true,

  "semi": true,

  "quoteProps": "as-needed",

  "jsxSingleQuote": true,

  "trailingComma": "all",

  "arrowParens": "always",

  "endOfLine": "lf",

  "bracketSpacing": true,

  "jsxBracketSameLine": true,

  "requirePragma": false,

  "insertPragma": false,

  "proseWrap": "preserve",

  "vueIndentScriptAndStyle": false

}
